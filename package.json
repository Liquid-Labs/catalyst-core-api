{
  "name": "@liquid-labs/catalyst-core-api",
  "version": "1.0.0-prototype.4",
  "description": "Core backend Catalyst components.",
  "directories": {
    "doc": "docs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "files": [
    "go/",
    "data/",
    "bin/",
    "dist/",
    "go.mod",
    "go.sum",
    ".catalyst"
  ],
  "bin": {
    "cloud-sql-proxy.sh": "./bin/cloud-sql-proxy.sh",
    "cloud-sql-service.sh": "./bin/cloud-sql-service.sh",
    "cloud_sql_proxy": "./bin/cloud_sql_proxy",
    "go-dev-server.sh": "./bin/go-dev-server.sh"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist/* && mkdir -p ./dist",
    "build": "catalyst-scripts build",
    "prepare": "npm run build",
    "pretest": "TEST_TYPES=unit catalyst-scripts pretest",
    "test": "TEST_TYPES=unit catalyst-scripts test",
    "postversion": "git push && git push --tags",
    "lint": "catalyst-scripts lint",
    "lint-fix": "catalyst-scripts lint-fix",
    "watch": "catalyst-scripts watch",
    "postyalc": "chmod a+x ~/.yalc/packages/@liquid-labs/catalyst-core-api/*/bin/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liquid-Labs/catalyst-core-api.git"
  },
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liquid-Labs/catalyst-core-api/issues"
  },
  "homepage": "https://github.com/Liquid-Labs/catalyst-core-api#readme",
  "catalyst": {
    "version": "0.0.0",
    "rollupConfig": {
      "commonjsConfig": {
        "namedExports": {
          "node_modules/react/index.js": [
            "useEffect",
            "useState"
          ]
        }
      }
    },
    "jestConfig": {
      "testEnvironment": "jsdom"
    }
  },
  "_catServices": [
    {
      "name": "cloud-sql-remote",
      "interface-classes": [
        "sql",
        "sql-mysql"
      ],
      "platform-types": [
        "local",
        "gcp"
      ],
      "purposes": [
        "dev",
        "pre-production"
      ],
      "ctrl-scripts": [
        "cloud-sql-proxy.sh",
        "cloud-sql-service.sh"
      ],
      "params-req": [
        "GCP_PROJECT_ID",
        "CLOUDSQL_INSTANCE_NAME",
        "CLOUDSQL_CONNECTION_PROT",
        "CLOUDSQL_CONNECTION_NAME",
        "CLOUDSQL_PROXY_CONNECTION_NAME",
        "CLOUDSQL_CREDS",
        "CLOUDSQL_DB",
        "FIREBASE_CREDS_FILE"
      ],
      "params-opt": [],
      "config-const": {}
    },
    {
      "name": "go-dev-server",
      "interface-classes": [
        "rest"
      ],
      "platform-types": [
        "local"
      ],
      "purposes": [
        "dev",
        "test"
      ],
      "ctrl-scripts": [
        "go-dev-server.sh"
      ],
      "params-req": [],
      "params-opt": [],
      "config-const": {}
    }
  ],
  "devDependencies": {
    "@liquid-labs/catalyst-scripts": "^1.0.0-alpha.16",
    "@liquid-labs/lock-and-key": "0.0.4",
    "@liquid-labs/react-waiter": "^1.0.0-prototype.10",
    "@liquid-labs/regex-repo": "latest",
    "@liquid-labs/restful-paths": "file:.yalc/@liquid-labs/restful-paths",
    "fetch-mock": "^7.3.0",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "lodash.upperfirst": "^4.3.1",
    "moment-timezone": "^0.5.23",
    "node-fetch": "^2.3.0",
    "normalizr": "^3.3.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-testing-library": "^5.8.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "peerDependencies": {
    "@liquid-labs/react-await": "^1.0.0-prototype.6",
    "@liquid-labs/react-waiter": "^1.0.0-prototype.10",
    "@liquid-labs/regex-repo": "latest",
    "@liquid-labs/restful-paths": "^1.0.0-prototype.0",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "lodash.upperfirst": "^4.3.1",
    "moment-timezone": "^0.5.23",
    "normalizr": "^3.3.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "dependencies": {}
}
